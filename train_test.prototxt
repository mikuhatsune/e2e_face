layer {
  name: "r/res5_6"
  type: "Eltwise"
  bottom: "res5_4"
  bottom: "conv5_6"
  top: "res5_6"
  eltwise_param { 
    operation: 1
  }
}
layer {
  name: "r/fc5"
  type: "InnerProduct"
  bottom: "res5_6"
  top: "fc5"
  param { lr_mult: 1 decay_mult: 1 }
  param { lr_mult: 2 decay_mult: 0 }
  inner_product_param {
    num_output: 512
    weight_filler { type: "xavier" }
  }
}

layer {
  name: "loss"
  type: "MarginSoftmaxLoss"
  bottom: "fc5"
  bottom: "Id"
  top: "loss"
  param { lr_mult: 1 decay_mult: 1 }
  margin_softmax_loss_param {
    num_output: 10575
    #lambda: 1000
    lambda {
      policy: STEPUP
      value: 1000
      gamma: 0.31 #0.1
      step: 1500
      max: 1000
      min: 8
      #iter: 0
    }
    weight_filler {
      type: "xavier"
      #type: "gaussian" std: 0.03
    }
  }
  include { phase: TRAIN }
}

