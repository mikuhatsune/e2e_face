name: "face-5-res-10575"
layer {
 name: "data"
 type: "HDF5Data"
 top: "I"
 top: "Id"
 include { phase: TRAIN }
 hdf5_data_param {
   source: "data/webface.txt"
   batch_size: 256
   shuffle: true
   scale: 0.00390625
   #crop_size: 120
   mirror: true
 }
}
layer {
  name: "data"
  type: "Input"
  top: "I"
  input_param { shape: { dim: 2 dim: 1 dim: 128 dim: 128 } }
  include { phase: TEST }
}

########## Localization Net ##########

layer {
  name: "tform/conv1"
  type: "Convolution"
  bottom: "I"
  top: "tform/conv1"
  param { lr_mult: 0.001 decay_mult: 0.001 }
  param { lr_mult: 0.002 decay_mult: 0 }
  convolution_param {
    num_output: 24
    kernel_size: 5
    stride: 1
    pad: 0
    weight_filler { type: "xavier" }
  }
}# 124
layer {
  name: "tform/pool1"
  type: "Pooling"
  bottom: "tform/conv1"
  top: "tform/pool1"
  pooling_param {
    pool: MAX
    kernel_size: 2
    stride: 2
  }
}# 62
layer {
  name: "tform/relu1"
  type: "PReLU"
  bottom: "tform/pool1"
  top: "tform/pool1"
  param { lr_mult: 0.001 decay_mult: 0 }
  #param { lr_mult: 0.0 decay_mult: 0 }
}

layer {
  name: "tform/conv2"
  type: "Convolution"
  bottom: "tform/pool1"
  top: "tform/conv2"
  param { lr_mult: 0.001 decay_mult: 0.001 }
  param { lr_mult: 0.002 decay_mult: 0 }
  convolution_param {
    num_output: 48
    kernel_size: 3
    stride: 1
    pad: 0
    weight_filler { type: "xavier" }
  }
}# 60
layer {
  name: "tform/pool2"
  type: "Pooling"
  bottom: "tform/conv2"
  top: "tform/pool2"
  pooling_param {
    pool: MAX
    kernel_size: 2
    stride: 2
  }
}# 30
layer {
  name: "tform/relu2"
  type: "PReLU"
  bottom: "tform/pool2"
  top: "tform/pool2"
  param { lr_mult: 0.001 decay_mult: 0 }
  #param { lr_mult: 0.0 decay_mult: 0 }
}

layer {
  name: "tform/conv3"
  type: "Convolution"
  bottom: "tform/pool2"
  top: "tform/conv3"
  param { lr_mult: 0.001 decay_mult: 0.001 }
  param { lr_mult: 0.002 decay_mult: 0 }
  convolution_param {
    num_output: 96
    kernel_size: 3
    stride: 1
    pad: 0
    weight_filler { type: "xavier" }
  }
}# 28
layer {
  name: "tform/pool3"
  type: "Pooling"
  bottom: "tform/conv3"
  top: "tform/pool3"
  pooling_param {
    pool: MAX
    kernel_size: 2
    stride: 2
  }
}# 14
layer {
  name: "tform/relu3"
  type: "PReLU"
  bottom: "tform/pool3"
  top: "tform/pool3"
  param { lr_mult: 0.001 decay_mult: 0 }
  #param { lr_mult: 0.0 decay_mult: 0 }
}

layer {
  name: "tform/ip1"
  type: "InnerProduct"
  bottom: "tform/pool3"
  top: "tform/ip1"
  param { lr_mult: 0.001 decay_mult: 0.001 }
  param { lr_mult: 0.002 decay_mult: 0 }
  inner_product_param {
    num_output: 64
    weight_filler {
      type: "gaussian"
      std: 0.03
    }
  }
}
layer {
  name: "tform/ip1r"
  type: "PReLU"
  bottom: "tform/ip1"
  top: "tform/ip1"
  param { lr_mult: 0.001 decay_mult: 0 }
  #param { lr_mult: 0.0 decay_mult: 0 }
}
layer {
  name: "tform/ip2"
  type: "InnerProduct"
  bottom: "tform/ip1"
  top: "tform/ip2"
  param { lr_mult: 0.001 decay_mult: 0.001 }
  param { lr_mult: 0.002 decay_mult: 0 }
  inner_product_param {
    num_output: 64
    weight_filler { type: "gaussian" std: 0.01 }
  }
}
layer {
  name: "tform/ip2r"
  type: "PReLU"
  bottom: "tform/ip2"
  top: "tform/ip2"
  param { lr_mult: 0.001 decay_mult: 0 }
  #param { lr_mult: 0.0 decay_mult: 0 }
}
layer {
  name: "tform"
  type: "InnerProduct"
  bottom: "tform/ip2"
  top: "tform"
  param { lr_mult: 0.001 decay_mult: 0.001 }
  param {
    lr_mult: 0.001 # !!
    decay_mult: 0.002
  }
  inner_product_param {
    num_output: 8
    weight_filler {
      #type: "constant"
      #value: 0
      type: "gaussian"
      std: 0.01
    }
    bias_filler {
      #type: "file" file: "bias_init.txt"
      type: "value"
      values: 0.7 values: 0   values: 0
      values: 0   values: 0.7 values: 0
      values: 0   values: 0
    }
  }
}


########## Sampler ##########
layer {
  name: "st"
  type: "SpatialTransformer"
  bottom: "I"
  bottom: "tform"
  top: "st_output"
  st_param {
    output_H: 96
    output_W: 96
    transform_type: "projective"
    #fill_value: 0.3867
  }
}



